<!doctype html>
<html lang="fr">
<head>
	<meta charset="utf-8">
	<title> Gestion d'Ã‰quipe </title>	 
	<link rel = "stylesheet" href="/css/style.css"> 
</head>
<body>
  <div class="header">
    <h1>Kibubu Rugby Club</h1>
        <div class="menu">
            <div> <a href="/">Accueil</a> </div>
            <div> <a href="https://www.sportkipik.be/-regionales-">Saison 2020-2021</a> </div>
            <div> <a href="http://www.kibubu.be">Le Club</a> </div>
        </div>
  </div>
  <div class="content">
      <!-- Liste des Joueurs --> 
      <div class="liste_des_joueurs"> 
        <ul>
          <% for (let i = 0 ; i < joueurs.length ; i++) { %> 
            <li> 
              <div class="joueurs">
                <%= joueurs[i].prenom %> <%= joueurs[i].nom %> 
              </div> 
              <div class="bouttons">    
                <a href="/liste_des_joueurs/supprimer/<%= joueurs[i].id_joueur %>"> Supprimer </a>
                <a href="/liste_des_joueurs/modifier/<%= joueurs[i].id_joueur %>"> Modifier </a>
              </br></br>
            </div>   
            </li>   
          <% } %>
        </ul>
    </div>
  </div>    
  <div class="footer">
  <div class="rs">
    <div class="insta">
      <a href="https://www.instagram.com/kibubu_rugby/">
        <div class="logo">
           <img src="rs/insta.jpg" alt="Lien Instagram" /> 
         </div>	
      </a>
    </div>
    <div class="face"> 
      <a href="https://www.facebook.com/KibubuRugbyClub">
        <div class="logo">
           <img src="rs/face.jpg" alt="Lien Facebook" /> 
         </div> 
      </a>
    </div>
  </div>
  </div>	    
</body>  


